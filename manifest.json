{
    "manifest_version": 3,
    "name": "My Extension",
    "version": "1.0",
    "description": "This is my extension",
    "icons": {
    },
    "action": {
        "default_icon": {
        }
    },
    "permissions": ["activeTab", "scripting"],
    "background": {
        "service_worker": "background.js"
    },
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+B",
                "mac": "Command+B"
            },
            "description": "Execute action"
        },
        "contrastFX": {
            "suggested_key": {
                "default": "Ctrl+Shift+1",
                "mac": "Command+Shift+1"
            },
            "description": "Contrast FX"
        },
        "cursorFinder": {
            "suggested_key": {
                "default": "Ctrl+Shift+Space",
                "mac": "Command+Shift+Space"
            },
            "description": "Cursor Finder"
        }
    
    }
}
